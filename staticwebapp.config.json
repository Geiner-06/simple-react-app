{
    "auth": {
        "identityProviders": {
            "github": {
                "registration": {
                    "clientId": "GITHUB_CLIENT_ID",
                    "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
                }
            }
        }
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ]
}